{
    "manifest_version": 2,
    "name": "Social Fuse",
    "description": "Quick launch the Social Media Accounts in just one click",
    "version": "1.0.0",
    "icons": { "128": "Social_Fuse_icon_128.png" },
    "browser_action": {
        "default_icon": "Social_Fuse_icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}